{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/screens/*": [
        "src/screens/*"
      ],
      "@/services/*": [
        "src/services/*"
      ],
      "@/hooks/*": [
        "src/hooks/*"
      ],
      "@/contexts/*": [
        "src/contexts/*"
      ],
      "@/navigation/*": [
        "src/navigation/*"
      ],
      "@/constants/*": [
        "src/constants/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ],
      "@/assets/*": [
        "src/assets/*"
      ]
    },
    "types": [
      "react-native",
      "@types/react"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
